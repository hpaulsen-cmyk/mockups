{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "ALLOW-FROM https://miro.com" },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://miro.com https://*.miro.com;"
        }
      ]
    }
  ]
}
